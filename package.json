{
  "name": "@ng-techpromux-archetype-project/apps-ui",
  "version": "0.0.0",
  "license": "MIT",
  "config": {
    "coreui_library_short_version": "4.2",
    "coreui_library_docs_url": "https://coreui.io/angular/docs/"
  },
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "serve:mydeviceinfo:prod": "npx nx serve mydeviceinfo --c=prod",
    "serve:mydeviceinfo:pre": "npx nx serve mydeviceinfo --c=pre",
    "serve:mydeviceinfo:dev": "npx nx serve mydeviceinfo --c=dev",
    "check:mydeviceinfo:fix": "eslint apps/mydeviceinfo/src --ext .ts --fix",
    "check:mydeviceinfo:prod": "eslint apps/mydeviceinfo/src --ext .ts && ng build --c=prod",
    "build:mydeviceinfo:prod": "npx nx build mydeviceinfo --c=prod --aot --base-href ./",
    "build:mydeviceinfo:pre": "npx nx build mydeviceinfo --c=pre --aot --base-href ./",
    "build:mydeviceinfo:dev": "npx nx build mydeviceinfo --c=dev --aot --base-href ./",
    "copy-assets-to-dist:mydeviceinfo": "touch \".npmrc\" && cpx \".npmrc\" \"dist/apps/mydeviceinfo/\" && cpx \"README.md\" \"dist/apps/mydeviceinfo/\" && cpx \"apps/mydeviceinfo/package.json\" \"dist/apps/mydeviceinfo/\"",
    "version:mydeviceinfo": "cd \"dist/apps/mydeviceinfo/\" && npm version",
    "publish:mydeviceinfo": "cd \"dist/apps/mydeviceinfo/\" && npm publish",
    "pack:mydeviceinfo": "cd \"dist/apps/mydeviceinfo/\" && npm pack",
    "postinstall": "node ./decorate-angular-cli.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/forms": "^15.2.10",
    "@angular/language-service": "^15.2.10",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/router": "^15.2.10",
    "@capacitor/android": "^6.2.1",
    "@capacitor/app": "^6.0.2",
    "@capacitor/browser": "^6.0.5",
    "@capacitor/core": "^6.2.1",
    "@capacitor/device": "^6.0.2",
    "@capacitor/dialog": "^6.0.2",
    "@capacitor/haptics": "^6.0.2",
    "@capacitor/ios": "^6.2.1",
    "@capacitor/keyboard": "^6.0.3",
    "@capacitor/status-bar": "^6.0.2",
    "@coreui/angular": "4.2.18",
    "@coreui/angular-chartjs": "4.2.15",
    "@coreui/chartjs": "3.0.0",
    "@coreui/coreui": "4.2.4",
    "@coreui/icons": "2.1.0",
    "@coreui/icons-angular": "4.2.15",
    "@coreui/utils": "1.3.1",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "angular-disable-browser-back-button": "^1.0.2",
    "bootstrap": "^5.3.6",
    "bootstrap-icons": "^1.13.1",
    "chart.js": "^4.4.9",
    "jquery": "^3.7.1",
    "ngx-perfect-scrollbar": "^10.1.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.8.1",
    "zone.js": "^0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.11",
    "@angular-eslint/eslint-plugin": "~14.0.4",
    "@angular-eslint/eslint-plugin-template": "~14.0.4",
    "@angular-eslint/template-parser": "~14.0.4",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "^15.0.0",
    "@angular/localize": "^15.2.10",
    "@capacitor/cli": "^6.2.1",
    "@nrwl/angular": "15.2.1",
    "@nrwl/cli": "15.2.1",
    "@nrwl/cypress": "15.2.1",
    "@nrwl/eslint-plugin-nx": "15.2.1",
    "@nrwl/jest": "15.2.1",
    "@nrwl/linter": "15.2.1",
    "@nrwl/workspace": "15.2.1",
    "@types/jasmine": "~4.3.0",
    "@types/jest": "28.1.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "cpx": "^1.5.0",
    "cypress": "^11.2.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.15.2",
    "jasmine-core": "~4.5.0",
    "jest": "28.1.1",
    "jest-environment-jsdom": "28.1.1",
    "jest-preset-angular": "~12.2.2",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "nx": "15.2.1",
    "prettier": "^2.8.8",
    "ts-jest": "28.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.8.2"
  }
}
